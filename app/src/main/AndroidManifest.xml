<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luo.matrixcaculator">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Analysis"
            android:label="Analysis"></activity>
        <activity android:name=".FourCompute"
            android:label="四阶矩阵计算"></activity>
        <activity android:name=".TwoCompute"
            android:label="二阶矩阵计算"></activity>
        <activity android:name=".ThreeCompute"
            android:label="三阶矩阵计算"></activity>
        <activity android:name=".MainActivity"
            android:label="矩阵计算">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".robo.ChatMainActivity"
            android:label="罗小炮">

        </activity>
    </application>

</manifest>
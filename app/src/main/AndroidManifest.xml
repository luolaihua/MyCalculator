<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.luo.matrixcaculator">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/tutut"
        android:label="@string/app_name"
        android:roundIcon="@drawable/g4"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main2"></activity>
        <activity android:name=".Setting" />
        <activity android:name=".CounterActivity" />
        <activity
            android:name=".Analysis"
            android:label="Analysis" />
        <activity
            android:name=".FourCompute"
            android:label="四阶矩阵计算" />
        <activity
            android:name=".TwoCompute"
            android:label="二阶矩阵计算" />
        <activity
            android:name=".ThreeCompute"
            android:label="三阶矩阵计算" />
        <activity
            android:name=".MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".robo.ChatMainActivity"
            android:label="罗小炮"
            android:windowSoftInputMode="adjustPan|stateHidden" />
    </application>

</manifest>